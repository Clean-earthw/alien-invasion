  "classes": {
    "wave-panel": {
      "content": {
        "width": "320px",
        "height": "380px",
        "background": "linear-gradient(135deg, rgba(30, 0, 50, 0.95), rgba(50, 0, 80, 0.95))",
        "borderRadius": 20,
        "padding": "25px",
        "border": "2px solid #ff00ff",
        "boxShadow": "0 0 30px rgba(255, 0, 255, 0.3)",
        "backdropFilter": "blur(10px)",
        "flexDirection": "column"
      }
    },
    "wave-header": {
      "content": {
        "width": "100%",
        "flexDirection": "row",
        "alignItems": "center",
        "justifyContent": "center",
        "gap": "15px",
        "marginBottom": "25px"
      }
    },
    "wave-icon": {
      "content": {
        "fontSize": "36px"
      }
    },
    "wave-title": {
      "content": {
        "fontSize": "26px",
        "fontWeight": "bold",
        "color": "#ffffff",
        "textTransform": "uppercase",
        "letterSpacing": "2px",
        "background": "linear-gradient(90deg, #ff00ff, #00ffff)",
        "WebkitBackgroundClip": "text",
        "WebkitTextFillColor": "transparent"
      }
    },
    "wave-progress-container": {
      "content": {
        "width": "100%",
        "marginBottom": "25px"
      }
    },
    "wave-progress-track": {
      "content": {
        "width": "100%",
        "height": "10px",
        "background": "rgba(255, 255, 255, 0.1)",
        "borderRadius": 5,
        "overflow": "hidden",
        "marginBottom": "10px"
      }
    },
    "wave-progress-fill": {
      "content": {
        "height": "100%",
        "background": "linear-gradient(90deg, #ff00ff, #8800ff)",
        "borderRadius": 5,
        "transition": "width 0.5s ease"
      }
    },
    "wave-progress-text": {
      "content": {
        "fontSize": "16px",
        "color": "#ffffff",
        "textAlign": "center",
        "fontWeight": "bold"
      }
    },
    "enemy-info": {
      "content": {
        "width": "100%",
        "background": "rgba(255, 0, 255, 0.2)",
        "borderRadius": 10,
        "padding": "15px",
        "marginBottom": "25px",
        "border": "2px solid rgba(255, 0, 255, 0.4)"
      }
    },
    "enemy-label": {
      "content": {
        "fontSize": "20px",
        "color": "#ffffff",
        "textAlign": "center",
        "fontWeight": "bold",
        "textTransform": "uppercase",
        "letterSpacing": "1px"
      }
    },
    "wave-stats": {
      "content": {
        "width": "100%",
        "flexDirection": "row",
        "justifyContent": "space-between",
        "marginBottom": "25px"
      }
    },
    "wave-stat": {
      "content": {
        "flexDirection": "column",
        "alignItems": "center",
        "gap": "8px"
      }
    },
    "wave-stat-icon": {
      "content": {
        "fontSize": "24px"
      }
    },
    "wave-stat-value": {
      "content": {
        "fontSize": "14px",
        "color": "#ffffff",
        "fontWeight": "bold"
      }
    },
    "next-wave-info": {
      "content": {
        "width": "100%",
        "flexDirection": "row",
        "justifyContent": "space-between",
        "padding": "15px",
        "background": "rgba(0, 255, 255, 0.1)",
        "borderRadius": 10,
        "border": "1px solid rgba(0, 255, 255, 0.3)"
      }
    },
    "next-wave-label": {
      "content": {
        "fontSize": "16px",
        "color": "#88ffff"
      }
    },
    "next-wave-value": {
      "content": {
        "fontSize": "18px",
        "color": "#00ffff",
        "fontWeight": "bold"
      }
    }
