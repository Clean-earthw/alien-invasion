  "classes": {
    "tutorial-panel": {
      "content": {
        "width": "600px",
        "height": "500px",
        "background": "linear-gradient(135deg, rgba(0, 0, 0, 0.95), rgba(20, 0, 40, 0.95))",
        "borderRadius": 25,
        "padding": "30px",
        "border": "3px solid #00ff88",
        "boxShadow": "0 0 50px rgba(0, 255, 136, 0.4)",
        "backdropFilter": "blur(15px)",
        "flexDirection": "column"
      }
    },
    "tutorial-header": {
      "content": {
        "width": "100%",
        "flexDirection": "row",
        "alignItems": "center",
        "justifyContent": "space-between",
        "marginBottom": "30px"
      }
    },
    "tutorial-icon": {
      "content": {
        "fontSize": "40px"
      }
    },
    "tutorial-title": {
      "content": {
        "fontSize": "32px",
        "fontWeight": "bold",
        "color": "#00ff88",
        "textTransform": "uppercase",
        "letterSpacing": "2px"
      }
    },
    "close-btn": {
      "content": {
        "width": "50px",
        "height": "50px",
        "background": "rgba(255, 0, 0, 0.2)",
        "border": "2px solid #ff0000",
        "borderRadius": 25,
        "alignItems": "center",
        "justifyContent": "center",
        "cursor": "pointer",
        "transition": "all 0.2s ease",
        "hover": {
          "background": "rgba(255, 0, 0, 0.4)",
          "transform": "scale(1.1)"
        }
      }
    },
    "close-icon": {
      "content": {
        "fontSize": "24px",
        "color": "#ffffff",
        "fontWeight": "bold"
      }
    },
    "tutorial-steps": {
      "content": {
        "width": "100%",
        "flexDirection": "column",
        "gap": "20px",
        "marginBottom": "30px",
        "flexGrow": "1"
      }
    },
    "tutorial-step": {
      "content": {
        "width": "100%",
        "flexDirection": "row",
        "alignItems": "center",
        "gap": "20px",
        "padding": "20px",
        "background": "rgba(255, 255, 255, 0.05)",
        "borderRadius": 15,
        "borderLeft": "5px solid #00ff88"
      }
    },
    "step-icon": {
      "content": {
        "fontSize": "36px",
        "minWidth": "60px",
        "textAlign": "center"
      }
    },
    "step-content": {
      "content": {
        "flexDirection": "column",
        "flexGrow": "1"
      }
    },
    "step-title": {
      "content": {
        "fontSize": "20px",
        "color": "#ffffff",
        "fontWeight": "bold",
        "marginBottom": "5px"
      }
    },
    "step-description": {
      "content": {
        "fontSize": "16px",
        "color": "#cccccc",
        "lineHeight": "1.4"
      }
    },
    "control-tips": {
      "content": {
        "width": "100%",
        "flexDirection": "column",
        "gap": "10px",
        "padding": "20px",
        "background": "rgba(0, 255, 136, 0.1)",
        "borderRadius": 15,
        "border": "2px solid rgba(0, 255, 136, 0.3)"
      }
    },
    "tip": {
      "content": {
        "fontSize": "16px",
        "color": "#88ffaa",
        "textAlign": "center",
        "fontWeight": "bold"
