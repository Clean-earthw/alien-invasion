  "classes": {
    "score-panel": {
      "content": {
        "width": "320px",
        "height": "380px",
        "background": "linear-gradient(135deg, rgba(0, 10, 30, 0.95), rgba(0, 20, 40, 0.95))",
        "borderRadius": 20,
        "padding": "25px",
        "border": "2px solid #0088ff",
        "boxShadow": "0 0 30px rgba(0, 136, 255, 0.3)",
        "backdropFilter": "blur(10px)",
        "flexDirection": "column"
      }
    },
    "score-header": {
      "content": {
        "width": "100%",
        "borderBottom": "2px solid rgba(0, 136, 255, 0.5)",
        "paddingBottom": "15px",
        "marginBottom": "20px"
      }
    },
    "score-title": {
      "content": {
        "fontSize": "22px",
        "fontWeight": "bold",
        "color": "#ffffff",
        "textAlign": "center",
        "letterSpacing": "1px",
        "textTransform": "uppercase"
      }
    },
    "stats-container": {
      "content": {
        "width": "100%",
        "flexDirection": "column",
        "gap": "15px",
        "marginBottom": "25px"
      }
    },
    "stat-row": {
      "content": {
        "width": "100%",
        "flexDirection": "row",
        "justifyContent": "space-between",
        "alignItems": "center",
        "padding": "12px 15px",
        "background": "rgba(0, 40, 80, 0.4)",
        "borderRadius": 10,
        "borderLeft": "4px solid"
      }
    },
    "stat-label": {
      "content": {
        "fontSize": "16px",
        "color": "#88ccff",
        "fontWeight": "bold"
      }
    },
    "stat-value": {
      "content": {
        "fontSize": "24px",
        "fontWeight": "bold",
        "fontFamily": "monospace",
        "textShadow": "0 0 10px currentColor"
      }
    },
    "health-container": {
      "content": {
        "width": "100%",
        "marginTop": "10px"
      }
    },
    "health-label": {
      "content": {
        "fontSize": "14px",
        "color": "#88ccff",
        "marginBottom": "8px",
        "textAlign": "center"
      }
    },
    "health-bar": {
      "content": {
        "width": "100%",
        "height": "12px",
        "background": "rgba(255, 255, 255, 0.1)",
        "borderRadius": 6,
        "overflow": "hidden",
        "border": "1px solid rgba(0, 136, 255, 0.3)"
      }
    },
    "health-fill": {
      "content": {
        "height": "100%",
        "background": "linear-gradient(90deg, #00ff00, #0088ff)",
        "borderRadius": 6,
        "transition": "width 0.3s ease"
      }
    },
    "ammo-info": {
      "content": {
        "width": "100%",
        "flexDirection": "row",
        "justifyContent": "space-between",
        "marginTop": "20px",
        "paddingTop": "15px",
        "borderTop": "1px solid rgba(255, 255, 255, 0.2)"
      }
    },
    "bomb-label": {
      "content": {
        "fontSize": "14px",
        "color": "#88aaff",
        "fontWeight": "bold"
      }
    },
    "__id__score-value": {
      "content": {
        "color": "#ff0000"
      }
    },
