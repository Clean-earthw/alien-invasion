    "progress-fill": {
      "content": {
        "height": "100%",
        "background": "linear-gradient(90deg, #0088ff, #00ffff)",
        "borderRadius": 4,
        "width": "20%",
        "transition": "width 0.3s ease",
        "boxShadow": "0 0 10px rgba(0, 200, 255, 0.5)"
